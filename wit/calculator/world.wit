package docs:calculator@0.1.0;

interface res {
   resource res {
    constructor();
    write: func(x: u32);
    read: static func(x: res) -> res;
   }
}

interface calculate {
    resource handle;
    enum op {
        add,
    }
    eval-expression: func(op: op, x: u32, y: u32) -> u32;
    test: func(x: handle);
}

world calculator {
    export calculate;
    import res;
    import docs:adder/add@0.1.0;
}

world app {
    import calculate;
}
